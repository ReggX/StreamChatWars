{
  "$schema": "../schema/config_schema.json",
  "config_name": "Splody for Monthly Madness",
  "events": {
    "file_events": {
      "enabled": true
    },
    "hotkeys": {
      "failsafe": "Shift+Backspace",
      "accept_input": "F13",
      "random_action": "F14",
      "reset_teams": "F15",
      "random_delay_plus": "F17",
      "random_delay_minus": "F18"
    },
    "max_delay_random": 500,
    "step_delay_random": 5
  },
  "operators": [
    "$broadcaster",
    "reggx"
  ],
