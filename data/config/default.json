{
  "$schema": "../schema/config_schema.json",
  "config_name": "Default",
  "events": {
    "file_events": {
      "enabled": true
    },
    "hotkeys": {
      "failsafe": "Shift+Backspace",
      "accept_input": "F13",
      "random_action": "F14",
      "reset_teams": "F15",
      "random_delay_plus": "F17",
      "random_delay_minus": "F18"
    },
    "max_delay_random": 500,
    "step_delay_random": 5
  },
  "commands": {
    "prefix": "?",
    "mode": "all"
  },
  "operators": [
    "$broadcaster",
    "reggx"
  ],
  "sessionlog": {
    "enable_chatlog": true,
    "enable_consolelog": true,
    "enable_channelpointlog": true,
    "enable_periodic_dumps": true
  },
  "tts": {
    "enabled": false
  },
  "irc": {
    "host": "irc.chat.twitch.tv",
    "port": 6697,
    "message_interval": 3,
    "connection_timeout": 10,
    "join_rate_limit_amount": 18,
    "join_rate_limit_time": 11
  },
  "default_team_data": {
    "type": "Balancing",
    "queue_length": 10,
    "hidden": false,
    "joinable": true,
    "leavable": true,
    "exclusive": true,
    "use_random_inputs": false,
    "spam_protection": true,
    "channels": [
      "reggx"
    ],
    "user_whitelist": [],
    "user_blacklist": [],
    "actionset": {
      "type": "Duckgame_KB",
      "doc_url": "",
      "action_prefix": "+",
      "input_server": {
        "type": "local"
      }
    }
  },
  "teams": [
    {
      "name": "Blue",
      "actionset": {
        "player_index": 0,
        "allow_changing_macros": true,
        "macro_file": "data/macros/test.json",
        "persistent_macros": false,
        "input_server": {}
      }
    },
    {
      "name": "Red",
      "actionset": {
        "player_index": 1,
        "input_server": {}
      }
    }
  ]
}
